{
  "name": "@storehouse/mongoose",
  "private": false,
  "version": "4.0.0",
  "description": "Mongoose manager for @storehouse/core",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "doc": "typedoc",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "kaukau --require ts-node/register --config kaukau-src.js",
    "testlib": "kaukau --config kaukau-lib.js",
    "format": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --write ./src",
    "format:check": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --check ./src",
    "format:staged": "cross-env NODE_OPTIONS=--experimental-strip-types lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kisiwu/storehouse-mongoose.git"
  },
  "keywords": [
    "storehouse",
    "mongoose"
  ],
  "author": "demingongo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kisiwu/storehouse-mongoose/issues"
  },
  "homepage": "https://kisiwu.github.io/storehouse/mongoose/latest/",
  "dependencies": {
    "@novice1/logger": "^1.6.1",
    "@storehouse/core": "^2.1.0",
    "bson": "^7.1.1",
    "mongoose": "^9.1.5",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^17.3.0",
    "kaukau": "^4.2.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}